<h1>All Accopmlishments</h1>

<% @accomplishments.each do |acc| %>
  <div class="list-style">
    <div class="">
      <%= link_to acc.title, accomplishment_path(acc) %>
    </div>
    <div class="">
      <%= acc.time_of_accompmlishment %>
    </div>
  </div>
  <div class="effect">
    <%= acc.effect %>
  </div>
  <div class="effect">
    by: <%= link_to acc.user.full_name, user_path(acc.user) %>
  </div>
<% end %>
